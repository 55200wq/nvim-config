    "filetypes": ["c", "cc", "cpp", "c++", "objc", "objcpp"]
  }
},
"smartf.wordJump": false,
 "translator.engines": [
    "youdao"
  ],
  "diagnostic-languageserver.filetypes": {
    "sh": "shellcheck"
  },
  "diagnostic-languageserver.formatFiletypes": {
    "sh": "shfmt"
  },
  "sumneko-lua.enableNvimLuaDev": true,
  "Lua.telemetry.enable": true
}
